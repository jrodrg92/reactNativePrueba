{
  "name": "criptovikreactnative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "react-scripts start",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "electron-start": "electron ./src",
    "build-win": "electron-builder build --win --publish never",
    "deploy-win": "electron-builder build --win --publish always",
    "build-linux": "electron-builder build --linux --publish never",
    "deploy-linux": "electron-builder build --linux --publish always",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.13",
    "@chakra-ui/react": "^1.6.1",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "babel-jest": "^26.6.3",
    "electron-is-dev": "^2.0.0",
    "electron-updater": "^4.3.9",
    "framer-motion": "^4.1.16",
    "is-electron": "^2.2.0",
    "react": "17.0.1",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-native": "0.64.1",
    "react-native-web": "^0.16.3",
    "react-scripts": "^4.0.3",
    "ts-jest": "^26.5.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "electron": "^12.0.7",
    "electron-builder": "^22.10.5",
    "eslint": "7.14.0",
    "metro-react-native-babel-preset": "^0.64.0",
    "npm-lifecycle": "^3.1.5",
    "react-test-renderer": "17.0.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
